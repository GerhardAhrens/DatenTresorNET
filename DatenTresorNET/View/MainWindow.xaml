<Window
    x:Class="DatenTresorNET.View.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DatenTresorNET"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    Tag="{DynamicResource ResourceKey=IconExcelCtrl}"
    mc:Ignorable="d">
    <Window.Icon>
        <DrawingImage Drawing="{Binding Tag.Drawing, RelativeSource={RelativeSource AncestorType=Window}}" />
    </Window.Icon>
    <Grid />
</Window>
